<h1>All City Details</h1>

<form action="/allDetails" method="GET">
    <label>City Code</label>
    <input type="text" name="code" value="<%=cty_code%>"><br>
    <label>City Name</label>
    <input type="text" name="name" value="<%=cty_name%>"><br>
    <label>City Population</label>
    <input type="text" name="name" value="<%=population%>"><br>
    <label>By the Sea</label>
    <input type="text" name="name" value="<%=isCoastal%>"><br>
    <label>Area (KM)</label>
    <input type="text" name="name" value="<%=areaKM%>"><br>
    <br><br>
    <label>Country Code</label>
    <input type="text" name="name" value="<%=co_code%>"><br>
    <label>Country Name</label>
    <input type="text" name="name" value="<%=co_name%>"><br>
</form>

<a href="../">Home</a>